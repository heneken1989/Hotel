@model IEnumerable<RoomPropertyDetail>
@{
    ViewData["Title"] = "Room PropertyDetail Index";
}


<p>
    <a asp-action="Create">Tạo Property Detail</a>
</p>
<table class="table table-hover table-striped table-bordered">
    <thead>
        <tr>
            <th>Id </th>
            <th>Tên</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(a=>item.Id)</td>
                <td>
                    @Html.DisplayFor(a=>item.Detail)
                </td>


                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |

                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>


